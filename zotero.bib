@article{flemming2009,
  title = {Personal {{Coaching}} Im {{Schulfu\ss{}ball}}.},
  volume = {50},
  issn = {0176-5906},
  abstract = {Aufgrund der oft gro\ss{}en Leistungsunterschiede haben es SportlehrerInnen im schulischen Sportunterricht nicht immer leicht, den Unterrichtsalltag f\"ur alle gleicherma\ss{}en gedeihlich zu gestalten. Dies wird vor allem beim Sportspiel Fu\ss{}ball deutlich. Oft spielen dabei die technisch guten Spieler nur sich den Ball zu, wegen der Konkurrenzsituation bleiben die leistungsschw\"acheren Spieler und die M\"adchen auf der Strecke. Findet der Sportunterricht ungehindert so statt, bleiben in den Augen von Verf. wesentliche Felder der "Perspektivenvielfalt" dieser Sportart ungenutzt! Ziel des exemplarisch vorgestellten Unterrichtsansatzes ist es deshalb zu zeigen, wie es der Lehrkraft gelingen kann, leistungsstarke und leistungsschwache Sch\"ulerInnen so zum "kooperativen" Fu\ss{}ballspielen zu bewegen, dass alle quasi parit\"atisch an den Spielhandlungen beteiligt werden k\"onnen. Im Zentrum dieser Vorgehensweise, die Verf. "Personal Coaching" nennen, steht eine von den Sch\"ulerInnen eigenst\"andig vorgenommen Analyse des Spielverhaltens mittels Partnerbeobachtung. Lemmer.},
  number = {10},
  journal = {Sport Praxis},
  author = {Flemming, Silke and Gissel, Norbert},
  year = {2009},
  keywords = {Didaktik,Fußball,Schulsport,Sportunterricht,Unterrichtsgestaltung,Differenzierung,Fachdidaktik,Geschlecht,Gruppendynamik,Sportspiel},
  pages = {17-20}
}

